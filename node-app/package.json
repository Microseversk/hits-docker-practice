{
  "name": "node-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "loadtest": "node scripts/loadtest.js",
    "loadtest:quick": "node scripts/loadtest-quick.js",
    "loadtest:all": "node scripts/loadtest-all.js",
    "loadtest:report": "node scripts/loadtest-report.js"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-form-data": "^2.0.14",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mysql2": "^3.6.0",
    "pug": "2.0.0-beta11",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "autocannon": "^7.12.0",
    "cli-table3": "^0.6.5"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  }
}
